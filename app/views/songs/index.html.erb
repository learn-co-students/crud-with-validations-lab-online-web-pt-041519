<h1>Song Index</h1>
<table>
    <tbody>
    <tr>
        <th>Title</th>
        <th>Released</th>
        <th>Release Year</th>
        <th>Artist Name</th>
        <th>Genre</th>
    </tr>
    <tr>
        <% @songs.each do |s| %>
            <td><%= s.title %></td>
            <td><%= s.released %></td>
            <td><%= s.release_year %></td>
            <td><%= s.artist_name %></td>
            <td><%= s.genre %></td>
        <% end %>
    </tr>
    </tbody>
</table>